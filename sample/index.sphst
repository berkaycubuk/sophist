const fm = _.frontmatter({
    title: "This Year\'s Goal: 11 Months 11 Products",
});

_.h1(fm.title)

const files = await _.getFiles('blog');

console.log(files);

files.forEach(i => {
    if (!i.frontmatter) {
        return;
    }
    if (!i.frontmatter.title) {
        return;
    }
    _.html(`<a href="/${i.url}">${i.frontmatter.title}</a>`);
});